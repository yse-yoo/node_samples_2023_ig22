<h1>Item list</h1>
<table class="table">
    <tr>
        <th>Name</th>
        <th>Price</th>
    </tr>
    <% if (items) { %>
    <% items.forEach((value)=> { %>
    <tr>
        <td>
            <a href="/item/<%= value.id %>">
                <%= value.name %>
            </a>
        </td>
        <td>150</td>
    </tr>
    <% }) %>
    <% } %>
</table>